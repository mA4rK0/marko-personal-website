---
import NavbarLayout from "../ui/Navbar-Layout";
import Tes from "./Tes.astro";
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="../../src/assets/icon.svg" />

    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Encode+Sans+Expanded:wght@100;200;300;400;500;600;700;800;900&display=swap" rel="stylesheet" />
    <link rel="stylesheet" href="../../src/css/main.css" />
    <meta name="generator" content={Astro.generator} />
    <title>Juan Website</title>
  </head>
  <body>
    <NavbarLayout client:only="react" />
    <Tes />
    <slot name="home" />
    <slot name="portfolio" />
    <slot name="about" />
    <slot name="skill" />
    <slot name="contact" />
    <script>
      import { initAOS } from "../script/init-aos";
      initAOS();
    </script>
  </body>
</html>

<style>
  html,
  body {
    margin: 0;
    width: 100%;
    height: 100%;
  }
</style>
